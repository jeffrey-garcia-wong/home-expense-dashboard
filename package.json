{
  "name": "home-expenses-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^3.9.1",
    "faker": "5.5.3",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "rxjs": "^7.5.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "5.5.3",
    "@types/jest": "^27.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "mock:data": "npx tsc src/data/service/MockDataWriter.ts --outdir build && node ./build/MockDataWriter.js",
    "copy:data": "node -e \"const fs=require('fs'); fs.copyFileSync('./public/data-$env.yml','./public/data.yml');\"",
    "start:prd": "env=prd npm run copy:data && npm run start",
    "start:dev": "env=dev npm run copy:data && npm run start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
